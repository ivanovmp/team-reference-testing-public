name: Dinic CI

on:
  push:
    branches: [ "main" ]
  pull_request:
    branches: [ "main" ]

jobs:
  build:

    runs-on: ubuntu-latest

    steps:
    - uses: actions/checkout@v3
    - name: install
      run: python3 ./utils/prepare-firefox.sh
    - name: test-cf
      run: cd ./dinic/testing/marbles/ && python3 test.py
      
